---
layout: null
---
[

  {% for page in site.pages %}
   {
     {% if page.title != nil %}
        "title"    : "{{ page.title | escape }}",
        "category" : "{{ page.category }}",
        "tags"     : "{{ page.tags | join: ', ' }}",
        "url"      : "{{ site.baseurl }}{{ page.url }}",
        "date"     : "{{ page.date }}",
        "content"  : "{{ page.content | strip_html | strip_newlines | remove_chars | escape| replace: '\t', '' }}"
     {% endif %}
   } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]